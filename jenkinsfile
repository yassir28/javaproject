pipeline {
    agent any
    stages {
        stage('Preparation') { 
            steps{
                
                git 'https://github.com/yassir28/javaproject.git'

            }
        
        }
        stage('Build') {
            steps {
                sh"sudo javac app.java"
                sh "java HelloWorld"
            }
            
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
            
        }
    }
}
